      "type": "cpu",
      "key": "  󰧑",
      "keyColor": "bright_green"
    },
    {
      "type": "gpu",
      "key": "  󰍹",
      "keyColor": "red"
    },
    {
      "type": "memory",
      "key": "  󰍛",
      "keyColor": "bright_yellow"
    },
    "break",
    {
      "type": "custom",
      "format": "┗━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┛"
